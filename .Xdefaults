URxvt*font: xft:Bitstream Vera Sans Mono:pixelsize=13
URxvt*transparent:true
URxvt*shading:10
URxvt.perl-ext-common:  default,matcher
URxvt.urlLauncher:      /usr/bin/firefox
URxvt.matcher.button:   1
URxvt*scrollBar:        false
urxvt*termName:         rxvt
URxvt*antialias:          true
URxvt*hinting:            full
URxvt*foreground: white
URxvt*background:       rgba:0000/0000/0000/cccc
URxvt.transparent:   true
URxvt.tintColor:     black

! Normal
! color0    black
! color1    red
! color2    green
! color3    yellow
! color4    blue
! color5    purple
! color6    cyan
! color7    white

urxvt*color0:    #333333
urxvt*color1:    #FF0000
urxvt*color2:    #00FF00
urxvt*color3:    #FF0000
urxvt*color4:    #0099FF
urxvt*color5:    #FF00FF
urxvt*color6:    #FF9900
urxvt*color7:    #FFFFFF

! Light
! color8    black
! color9    red
! color10    green
! color11    yellow
! color12    blue
! color13    purple
! color14    cyan
! color15    white
!
! urxvt*color8:    #747474
! urxvt*color9:    #f99286
! urxvt*color10:    #c3f786
! urxvt*color11:    #fcfbcc
! urxvt*color12:    #b6defb
! urxvt*color13:    #fba1fb
! urxvt*color14:    #d7d9fc
